<h1><center>Assignment3 - Chat Application</center><h1>

Chat:
<br>
<span id=" chat"></span>
<hr>

Message:

<br>
<textarea id="msg" style="width:380px; height: 60px;"></textarea>
<hr>
<button id="send">Send Message</button>

<script>
  $(document).ready(function(){

      /*Recieved msg */
      App.Chat.received = function(data)
      {
          $("#chat").append.(data['message'])
      }

      $("send").click(function(){
        msg = $("msg").val()

        /* send msg */
        App.Chat.send_msg(msg)
      })
  })
  </script>
